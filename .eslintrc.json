{
    "env": {
        "jquery": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "globals": {
        "io": true
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "class-methods-use-this": "off",
        "no-restricted-syntax": "off",
        "no-else-return": "off",
        "max-len": ["error", {
            "code": 140
        }]
    }
}